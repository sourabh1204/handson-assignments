package com.hsbc.model.dao;

import java.util.ArrayList;

import com.hsbc.model.beans.Contact;
import com.hsbc.model.beans.Profile;

public interface AppDao {
	public Profile storeProfile(Profile profile);
	public Profile updateProfile(int profileId, Profile profile);
	public String deleteProfile();
	public Contact addContact(Contact contact);
	public Contact updateContact(int contactId, Contact contact);
	public ArrayList<Contact> fetchAll(int profileId);
	public String deleteContact(int contactId);
}
